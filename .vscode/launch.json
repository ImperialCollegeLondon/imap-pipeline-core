{
    "configurations": [
        {
            "console": "integratedTerminal",
            "justMyCode": false,
            "name": "Python: Debug Tests",
            "program": "${file}",
            "purpose": [
                "debug-test"
            ],
            "request": "launch",
            "type": "debugpy"
        },
        {
            "code": "import prefect_server.workflow; prefect_server.workflow.deploy_flows(local_debug=True)",
            "cwd": "${workspaceFolder}",
            // vscode will load .env file by default - so copy defaults.env to .env and edit locally
            //"envFile": "${workspaceFolder}/defaults.env",
            "name": "Python Debugger: Prefect deploy and run",
            "request": "launch",
            "type": "debugpy",
            "preLaunchTask": "start_servers"
        },
        {
            "args": [
                "process"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "name": "Python: Current File",
            "cwd": "${workspaceFolder}",
            "program": "${file}",
            "request": "launch",
            "type": "debugpy",
            // vscode will load .env file by default - so copy defaults.env to .env
            //"envFile": "${workspaceFolder}/defaults.env",
            "preLaunchTask": "start_servers"
        }
    ],
    "version": "0.2.0"
}
